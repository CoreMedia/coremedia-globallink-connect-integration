{
  "name": "@coremedia-labs/studio-client.workflow.gcc-studio-client",
  "author": "CoreMedia GmbH",
  "version": "1.0.0-SNAPSHOT",
  "license": "CoreMedia Open Source License",
  "type": "module",
  "private": true,
  "dependencies": {
    "@coremedia-labs/studio-client.ext.gcc-studio-client": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@coremedia/studio-client.build-config": "catalog:",
    "@coremedia/studio-client.eslint-config": "catalog:",
    "@coremedia/studio-client.workflow.shared-modules": "catalog:",
    "eslint": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "webpack-cli": "catalog:",
    "webpack": "catalog:"
  },
  "scripts": {
    "build": "tsc --project src && webpack build --mode=production",
    "clean": "rimraf ./dist",
    "lint": "eslint",
    "prepublishOnly": "prepare-package",
    "watch": "webpack watch --mode=development"
  },
  "exports": {
    "./appEntry.js": {
      "default": "./dist/appEntry.js"
    }
  },
  "coremedia": {
    "projectExtensionFor": "studio-client.workflow",
    "plugins": {
      "workflow_react_gccWorkflowPlugin": "./dist/appEntry.js"
    }
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "exports": {
      "./appEntry.js": {
        "default": "./appEntry.js"
      }
    },
    "coremedia": {
      "plugins": {
        "workflow_react_gccWorkflowPlugin": "./appEntry.js"
      }
    }
  }
}
