{
  "name": "@coremedia-labs/studio-client.workflow.gcc-studio-client",
  "author": "CoreMedia GmbH",
  "version": "1.0.0-SNAPSHOT",
  "license": "CoreMedia Open Source License",
  "private": true,
  "dependencies": {
    "@coremedia-labs/studio-client.ext.gcc-studio-client": "workspace:*"
  },
  "devDependencies": {
    "@jangaroo/build": "^3.2.0",
    "@jangaroo/core": "^3.2.0",
    "rimraf": "^5.0.7"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./build",
    "build": "jangaroo build",
    "watch": "jangaroo watch",
    "publish": "jangaroo publish"
  },
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/src/*.js"
    }
  },
  "coremedia": {
    "projectExtensionFor": "studio-client.workflow"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "exports": {
      "./*": {
        "types": "./src/*.d.ts",
        "default": "./src/*.js"
      }
    }
  }
}
