{
  "name": "@coremedia-labs/studio-client.main.gcc-studio-client",
  "author": "CoreMedia GmbH",
  "version": "1.0.0-SNAPSHOT",
  "license": "CoreMedia Open Source License",
  "private": true,
  "dependencies": {
    "@coremedia-labs/studio-client.ext.gcc-studio-client": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@coremedia/eslint-config-studio-client": "workspace:*",
    "@coremedia/studio-client.build-config": "workspace:*",
    "@coremedia/studio-client.main.shared-modules": "workspace:*",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.8.2",
    "webpack-cli": "^6.0.1",
    "webpack": "^5.98.0"
  },
  "scripts": {
    "build": "tsc --project src && webpack build --mode=production",
    "clean": "rimraf ./dist",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepublishOnly": "prepare-package",
    "watch": "webpack watch --mode=development"
  },
  "exports": {
    "./appEntry.js": {
      "default": "./dist/appEntry.js"
    }
  },
  "coremedia": {
    "projectExtensionFor": "studio-client.main",
    "plugins": {
      "main_gccWorkflowPlugin": "./dist/appEntry.js"
    }
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "exports": {
      "./appEntry.js": {
        "default": "./appEntry.js"
      }
    },
    "coremedia": {
      "plugins": {
        "main_gccWorkflowPlugin": "./appEntry.js"
      }
    }
  }
}
