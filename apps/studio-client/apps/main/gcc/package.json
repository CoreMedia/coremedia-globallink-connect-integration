{
  "name": "@coremedia-labs/studio-client.main.gcc-studio-client",
  "author": "CoreMedia GmbH",
  "version": "1.0.0-SNAPSHOT",
  "license": "CoreMedia Open Source License",
  "private": true,
  "dependencies": {
    "@coremedia-labs/studio-client.ext.gcc-studio-client": "workspace:*"
  },
  "devDependencies": {
    "@jangaroo/build": "^3.1.0",

    "@jangaroo/core": "^3.1.0",
    "@coremedia/eslint-config-studio-client-ext": "^2412.0.0",
    "@jangaroo/publish": "^3.0.1",
    "eslint": "^8.57.0",

    "rimraf": "^5.0.7"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./build",
    "build": "jangaroo build",
    "watch": "jangaroo watch",
    "publish": "jangaroo publish",
    "lint": "eslint --fix \"src/**/*.ts\""
  },
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/src/*.js"
    }
  },
  "coremedia": {
    "projectExtensionFor": "studio-client.main"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "exports": {
      "./*": {
        "types": "./src/*.d.ts",
        "default": "./src/*.js"
      }
    }
  }
}
