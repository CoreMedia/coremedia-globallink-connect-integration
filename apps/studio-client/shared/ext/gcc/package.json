    "@jangaroo/runtime": "^1.0.0-beta"
  },
  "devDependencies": {
    "@jangaroo/build": "^1.0.0-beta",
    "@jangaroo/core": "^1.0.0-beta",
    "@jangaroo/publish": "^1.0.0-beta",
    "eslint": "7.27.0",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./build",
    "build": "jangaroo build",
    "watch": "jangaroo watch",
    "publish": "jangaroo publish",
    "lint": "eslint --fix \"src/**/*.ts\""
  },
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "default": "./build/src/*.js"
    }
  },
  "publishConfig": {
    "directory": "build",
    "exports": {
      "./*": {
        "types": "./src/*.d.ts",
        "default": "./src/*.js"
      }
    }
  }
