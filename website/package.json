{
  "name": "website",
  "version": "1.0.0",
  "description": "GitHub Pages for CoreMedia GCC integration provided by Docusaurus.",
  "author": {
    "name": "CoreMedia GmbH",
    "email": "info@coremedia.com",
    "url": "https://coremedia.com/"
  },
  "engines": {
    "node": ">=22",
    "pnpm": "^10.9"
  },
  "private": true,
  "keywords": [
    "docusaurus",
    "globallink",
    "gcc",
    "coremedia",
    "coremedia cms",
    "coremedia cmcc"
  ],
  "scripts": {
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "deps:check": "depcheck",
    "deps:unused": "depcheck --json | jq '.dependencies + .devDependencies'",
    "docusaurus": "docusaurus",
    "preinstall": "node ./scripts/check-pnpm.mjs",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "serve": "docusaurus serve",
    "typecheck": "tsc",
    "update:latest": "pnpm update --interactive --latest --recursive",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/logger": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@mdx-js/react": "^3.0.0",
    "@types/mdast": "^4.0.4",
    "clsx": "^2.0.0",
    "mdast-util-from-markdown": "^2.0.2",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remark": "^15.0.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@types/unist": "^3.0.3",
    "depcheck": "^1.4.7",
    "typescript": "~5.9.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  }
}
